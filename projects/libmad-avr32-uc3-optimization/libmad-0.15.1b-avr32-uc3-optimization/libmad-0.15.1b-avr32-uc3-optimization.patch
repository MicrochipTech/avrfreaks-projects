+	st.d	r12[7*4], r4	/* y[7] = tmp[16], y[8] = tmp[17] */
+	neg	r5
+	neg	r4
+	st.w	r12[10*4], r5	/* y[10] = -tmp[16] */
+	st.w	r12[9*4], r4	/* y[9] = -tmp[17] */
+
