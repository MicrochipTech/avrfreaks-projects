	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
	;|		
	_PS2_DEVICE_KEYBOARD_DEC_G:
		push	AccH
		lds	AccH,_PS2_DEVICE_KEYBOARD_QUEUE_HANDDLE		;get keyboard data handdle
		rcall	_CIRCULAR_QUEUE_DEC_G				;G--
		pop	AccH
		ret
	;>--------------------------------------------------------------<
	;| OBJECT     : PS2_DEVICE					|
	;>--------------------------------------------------------------<
	;| FUNCTION   : PS2_DEVICE_KEYBOARD_PUT_DATA_QUEUE		|
	;>--------------------------------------------------------------<
	;| DESCRIPTION: Put data into PS2 KEYBOARD circular queue	|
	;>--------------------------------------------------------------<
	;| INPUT      :	Acc  data					|
	;| OUTPUT     :	None						|
	;| DESTROYED  :	None						|
	;| TIME	      :	----						|
