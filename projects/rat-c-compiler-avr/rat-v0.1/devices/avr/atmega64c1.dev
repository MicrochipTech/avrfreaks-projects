		AMP1EN = 7              ; 
	}
	AMP2CSR(0x77) {         ; 
		AMP2TS0 = 0             ; 
		AMP2TS1 = 1             ; 
		AMP2TS2 = 2             ; 
		AMPCMP2 = 3             ; Amplifier 2 - Comparator 2 Connection
		AMP2G0 = 4              ; 
		AMP2G1 = 5              ; 
		AMP2IS = 6              ; 
		AMP2EN = 7              ; 
	}
