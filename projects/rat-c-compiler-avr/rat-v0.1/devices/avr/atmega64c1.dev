		AMP0G0 = 4              ; 
		AMP0G1 = 5              ; 
		AMP0IS = 6              ; 
		AMP0EN = 7              ; 
	}
	AMP1CSR(0x76) {         ; 
		AMP1TS0 = 0             ; 
