	TIMER5_COMPA = 0x2f     ; Timer/Counter5 Compare Match A
	TIMER5_COMPB = 0x30     ; Timer/Counter5 Compare Match B
	TIMER5_COMPC = 0x31     ; Timer/Counter5 Compare Match C
	TIMER5_OVF = 0x32       ; Timer/Counter5 Overflow
