                 	_RTC_GET_ELAPSED:
003703 930f
003704 931f      		pushawT						;save used
003705 910d      		ld	AccT,X+					;get old ticks
003706 911d      		ld	AccTH,X+
003707 9712      		sbiw	X,2
003708 940e 36e9 		call	_RTC_GET_TICKS				;Get actual ticks
00370a 1b80      		sub	Acc,AccT				;subtract new-old 
00370b 0b91      		sbc	AccH,AccTH
00370c 911f
00370d 910f      		popawT
