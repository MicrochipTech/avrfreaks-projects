                 	_MOTION_RELAY_INIT:
00131d 938f      		push	Acc						;save useds
00131e b78f      		in	Acc,SREG					;save SREG
00131f 94f8      		cli							;disable interrupts during initialization
001320 938f
001321 939f      		pushaw
                 		;@----------------------@
                 		;| Relay port bit	|
                 		;@----------------------@
001322 9180 0065 		lds	Acc,_MOTION_RELAY_PORT_OUTPUT			;get old state
001324 7f8d      		andi	Acc,~(1<<_MOTION_RELAY_BIT)			;set RELAY OFF
001325 9380 0065 		sts	_MOTION_RELAY_PORT_OUTPUT,Acc
001327 9180 0064 		lds	Acc,_MOTION_RELAY_PORT_DIR			;get old state
001329 6082      		ori	Acc,(1<<_MOTION_RELAY_BIT)			;set as output
00132a 9380 0064 		sts	_MOTION_RELAY_PORT_DIR,Acc
                 		;@----------------------@
                 		;| Relay event		|
                 		;@----------------------@
00132c e085      		ldi	Acc,_MOTION_RELAY_EVENT_END			;set event end
00132d 9380 017f 		sts	_MOTION_RELAY_EVENT,Acc
00132f e088
001330 e090      		ldiaw	8						;set 8 ms (125Hz) RELAY_TIME_STEP
001331 9380 0182
001333 9390 0183 		stsaw	_MOTION_RELAY_TIME_STEP
001335 919f
001336 918f      		popaw							;restore SREG state
001337 bf8f      		out	SREG,Acc
001338 918f      		pop	Acc						;restore useds
001339 9508      		ret
                 
