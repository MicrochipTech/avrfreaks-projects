0012ca 918f      		pop	Acc						;restore log counter
0012cb 5081      		subi	Acc,1						;NUMBER OF BYTES--
0012cc 4090      		sbci	AccH,0
0012cd cff5      		rjmp	_EEPROM_R_STR_SRAM_10				;get next data *<NOCHANGE>*
                 	_EEPROM_R_STR_SRAM_DONE:
0012ce 910f      		pop	AccT						;restore useds
0012cf 919f
0012d0 918f      		popaw
0012d1 9508      		ret
