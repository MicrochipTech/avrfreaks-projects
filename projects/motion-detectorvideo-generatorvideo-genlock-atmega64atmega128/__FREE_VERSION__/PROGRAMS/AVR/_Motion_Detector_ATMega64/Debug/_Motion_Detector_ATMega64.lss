                 	_RTC_SET_ELAPSED:
0036f9 938f
0036fa 939f      		pushaw						;save useds
0036fb 940e 36e9 		call	_RTC_GET_TICKS				;get ticks
0036fd 938d      		st	X+,Acc					;save ticks
0036fe 939d      		st	X+,AccH	
0036ff 9712      		sbiw	X,2				
003700 919f
003701 918f      		popaw
003702 9508      		ret
