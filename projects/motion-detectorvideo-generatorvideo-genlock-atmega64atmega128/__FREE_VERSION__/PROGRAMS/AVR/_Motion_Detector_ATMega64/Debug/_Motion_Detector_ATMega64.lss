                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:							|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_RTC_GET_TICKS:
0036e9 930f
0036ea 931f      		pushawt						;save useds
                 	_RTC_GET_TICKS_10:
0036eb 9100 0ecc
0036ed 9110 0ecd 		ldsawt	_RTC_TICKS				;Get ticks
0036ef 9180 0ecc
0036f1 9190 0ecd 		ldsaw	_RTC_TICKS				;Get Ticks Again
0036f3 1780      		cp	Acc,AccT
0036f4 0791      		cpc	AccH,AccTH
0036f5 f7a9      		brne	_RTC_GET_TICKS_10			;branch until get correct tiks
0036f6 911f
0036f7 910f      		popawt						;restore useds
0036f8 9508      		ret	
