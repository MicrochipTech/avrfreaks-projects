                 		;////////////////////////////////
                 		;// ADJUST COLOR FOR 1 BIT
                 		;////////////////////////////////
                 	_RASTER_ADJUST_COLOR_MASK_10:	
00053e 7081      		andi	Acc,0x1					;clear bits 1..7
00053f fd80      		sbrc	Acc,0					;skip if bit 0=0
000540 6f8e      		ori	Acc,0xfe				;else set bit 1..7
000541 c00b      		rjmp	_RASTER_ADJUST_COLOR_MASK_40		;process the rest
                 		;////////////////////////////////
                 		;// ADJUST COLOR FOR 2 BIT
