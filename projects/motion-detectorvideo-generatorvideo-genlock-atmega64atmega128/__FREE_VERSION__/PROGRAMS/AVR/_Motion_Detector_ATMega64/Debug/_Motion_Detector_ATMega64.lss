                 ;>--------------------------------------------------------------<
                 ;| OBJECT     : BOOT						|
                 ;>--------------------------------------------------------------<
                 ;| DESCRIPTION: This object check boot state if HOT or COLD	|
                 ;>--------------------------------------------------------------<
                 
                 ;>----------------------<
                 ;| OBJECT DEFINITIONS	|
                 ;>----------------------<
                 
                 
                 ;-------------------------< PORT DEFINITONS >----------------------------
                 
                 
                 ;-------------------------< CONSTANT DEFINITONS >------------------------
                 
                 .EQU	_BOOT_SIGNATURE_1	= 27			;signature for hot boot byte 1
                 .EQU	_BOOT_SIGNATURE_2	= 18			;signature for hot boot byte 2
                 .EQU	_BOOT_SIGNATURE_3	= 28			;signature for hot boot byte 3
                 .EQU	_BOOT_RTC_SIGNATURE_OFF	= 0X5A			;signature for hot boot byte 4
                 
                 ;-------------------------< SRAM  DEFINITONS >------------------------
                 
                 .DSEG
00017b           	_BOOT_SIGNATURE_BYTE_1:	.BYTE	_BYTE		;if hot boot contents = 27
00017c           	_BOOT_SIGNATURE_BYTE_2:	.BYTE	_BYTE		;if hot boot contents = 18
00017d           	_BOOT_SIGNATURE_BYTE_3:	.BYTE	_BYTE		;if hot boot contents = 28
00017e           	_BOOT_RTC_STATUS:	.BYTE	_BYTE		;if not set contents = 0x5a
                 .CSEG
                 
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : BOOT						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : BOOT_STATE					|
                 	;>--------------------------------------------------------------<
                 	;| DESCRIPTION: CHECK BOOT STATE COLD OR HOT			|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	Acc  _ON=HOT _OFF=COLD				|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	----						|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_BOOT_GET_STATE:
0012d2 9180 017b 		lds	Acc,_BOOT_SIGNATURE_BYTE_1			;get signature byte 1
0012d4 318b      		cpi	Acc,_BOOT_SIGNATURE_1				;math ?	
0012d5 f451      		brne	_BOOT_GET_STATE_10				;no, branch
0012d6 9180 017c 		lds	Acc,_BOOT_SIGNATURE_BYTE_2			;get signature byte 2
0012d8 3182      		cpi	Acc,_BOOT_SIGNATURE_2				;math ?	
0012d9 f431      		brne	_BOOT_GET_STATE_10				;no, branch
0012da 9180 017d 		lds	Acc,_BOOT_SIGNATURE_BYTE_3			;get signature byte 3
0012dc 318c      		cpi	Acc,_BOOT_SIGNATURE_3				;math ?	
0012dd f411      		brne	_BOOT_GET_STATE_10				;no, branch
                 		;/////////////////
                 		;// HOT BOOT
                 		;/////////////////
0012de ef8f      		ldi	Acc,_ON
0012df 9508      		ret
                 		;/////////////////
                 		;// COLD BOOT
                 		;/////////////////
                 	_BOOT_GET_STATE_10:
0012e0 e18b      		ldi	Acc,_BOOT_SIGNATURE_1				;set signature bytes
0012e1 9380 017b 		sts	_BOOT_SIGNATURE_BYTE_1,Acc
0012e3 e182      		ldi	Acc,_BOOT_SIGNATURE_2
0012e4 9380 017c 		sts	_BOOT_SIGNATURE_BYTE_2,Acc
0012e6 e18c      		ldi	Acc,_BOOT_SIGNATURE_3
0012e7 9380 017d 		sts	_BOOT_SIGNATURE_BYTE_3,Acc
0012e9 e58a      		ldi	Acc,_BOOT_RTC_SIGNATURE_OFF			;set clock not set
0012ea 9380 017e 		sts	_BOOT_RTC_STATUS,Acc
0012ec e080      		ldi	Acc,_OFF
0012ed 9508      		ret
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : BOOT						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : BOOT_GET_RTC_STATE				|
                 	;>--------------------------------------------------------------<
                 	;| DESCRIPTION: CHECK RTC STATE SET OR NOT			|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	Acc  _ON=if set _OFF=not set			|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	----						|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_BOOT_GET_RTC_STATE:
0012ee 9180 017e 		lds	Acc,_BOOT_RTC_STATUS				;get RTC STATUS
0012f0 358a      		cpi	Acc,_BOOT_RTC_SIGNATURE_OFF			;not set?
0012f1 e080      		ldi	Acc,_OFF					;Assume yes
0012f2 f009      		breq	PC+2						;branch if yes
0012f3 ef8f      		ldi	Acc,_ON						;else ON
0012f4 9508      		ret
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : BOOT						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : BOOT_SET_RTC_OK					|
                 	;>--------------------------------------------------------------<
                 	;| DESCRIPTION: SET RTC STATE TO OK				|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	None			                        |
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	----						|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_BOOT_SET_RTC_OK:
0012f5 938f      		push	Acc
0012f6 e080      		ldi	Acc,0
0012f7 9380 017e 		sts	_BOOT_RTC_STATUS,Acc
0012f9 918f      		pop	Acc
0012fa 9508      		ret
