                 	;|/  			          		 |
                 	;|   	        		  		 |
                 	;+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+
                 	;   |  |  |  |  |  |  |  |  |  |  |  |  |  |  |
                 	;   \__\__\__\__\__\__\__\__\__\__\__\__\__\__\_________________________________________________________
                 	.MACRO	_M_DELAY_MILLISECONDS
                 		pushaw
                 		ldiaw	@0
