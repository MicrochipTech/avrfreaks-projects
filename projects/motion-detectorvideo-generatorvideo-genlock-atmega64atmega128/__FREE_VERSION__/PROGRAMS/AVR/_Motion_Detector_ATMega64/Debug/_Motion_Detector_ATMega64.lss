                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : RASTER						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : RASTER_CREATE					|
                 	;>--------------------------------------------------------------<
                 	;| DESCRIPTION: Create a new RASTER with Width and Height and	|
                 	;|		number of bits					|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	X	Width in pixels				|
                 	;|		Y	Height in pixels			|
                 	;|		Z-->	Memory area of raster			|
                 	;|		Acc	Raster bit size				|
                 	;| OUTPUT     :	None						|
                 	;| DESTROYED  :	Flags						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	valids bits sizes are 1,2,4 and 8 if other value|
                 	;|		is supplied a umpredictable result my occur,	|
                 	;|		area pointed by Z must be reserved space before	|
                 	;|		call this routine				|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	_RASTER_CREATE:
000509 939f      		push	AccH				;save useds
00050a 93af
00050b 93bf      		pushw	X
00050c 9380 0166 		sts	_RASTER_BIT_SIZE,Acc		;save bit size
00050e 93e0 0167
000510 93f0 0168 		stsw	_RASTER_START_ADDR,Z		;save start address
000512 93a0 0163 		sts	_RASTER_WIDTH,XL		;save width
000514 93c0 0165 		sts	_RASTER_HEIGHT,YL		;save height
                 		;//////////////////////////////////////////////
                 		;// compute size in bytes according bit size
                 		;//
                 		;// 1 bit - widthbyte=width/8
                 		;// 2 bit - widthbyte=width/4
                 		;// 4 bit - widthbyte=width/2
                 		;// 8 bit - widthbyte=width
                 		;//////////////////////////////////////////////
000516 e093      		ldi	AccH,3				;X>>3
000517 3081      		cpi	Acc,1				;1 bit size ?
000518 f039      		breq	_RASTER_CREATE_10		;yes, branch
000519 e092      		ldi	AccH,2				;X>>2
00051a 3082      		cpi	Acc,2				;2 bit size ?
00051b f021      		breq	_RASTER_CREATE_10		;yes, branch
00051c e091      		ldi	AccH,1				;X>>1
00051d 3084      		cpi	Acc,4				;4 bit size ?
00051e f009      		breq	_RASTER_CREATE_10		;yes, branch
00051f e090      		ldi	AccH,0				;else assume 8 bit size	
                 	_RASTER_CREATE_10:
000520 2399      		tst	AccH				;divide done ?
000521 f021      		breq	_RASTER_CREATE_20		;yes, branch
000522 95a6      		lsr	XL				;X=X/2
000523 95b7      		ror	XH
000524 959a      		dec	AccH
000525 cffa      		rjmp	_RASTER_CREATE_10
