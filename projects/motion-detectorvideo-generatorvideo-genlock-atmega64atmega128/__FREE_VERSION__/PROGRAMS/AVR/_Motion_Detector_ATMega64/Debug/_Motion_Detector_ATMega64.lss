                 ;>--------------------------------------------------------------<
                 ;| OBJECT     : RTC						|
                 ;>--------------------------------------------------------------<
                 ;| DESCRIPTION: This object emulate a Real Time Clock		|
                 ;>--------------------------------------------------------------<
                 
                 ;>----------------------<
                 ;| OBJECT DEFINITIONS	|
                 ;>----------------------<
                 
                 
                 ;-------------------------< PORT DEFINITONS >----------------------------
                 
                 
                 ;-------------------------< CONSTANT DEFINITONS >------------------------
                 
                 .EQU	_RTC_TIMER0_DIV_FACTOR	= 125-1				;factor to generate 125Hz timer0 interrupt
                 .EQU	_RTC_SECOND_DIV_FACTOR	= 125				;factor to generate 1 second timer
                 
                 ;-------------------------< SRAM  DEFINITONS >------------------------
                 
                 .DSEG
000ec8           	_RTC_DIV:		.BYTE	_BYTE			;RTC DIVISION FACTOR TO GENERATE 1 SECOND CLOCK
000ec9           	_RTC_SECOND:		.BYTE	_BYTE			;SECONDS
000eca           	_RTC_MINUTE:		.BYTE	_BYTE			;MINUTES
000ecb           	_RTC_HOUR:		.BYTE	_BYTE			;HOURS
000ecc           	_RTC_TICKS:		.BYTE	_WORD			;RTC TICK (125 TICKS PER SECOND)
                 .CSEG
                 
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : RTC						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : RTC_INIT					|
                 	;>--------------------------------------------------------------<
                 	;| Description: Initialize RTC engine				|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	None						|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	Disable interrupts during initialization	|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_RTC_INIT:
003671 938f      		push	Acc						;save useds
003672 b78f      		in	Acc,SREG					;save SREG
003673 94f8      		cli							;disable interrupts during initialization
003674 938f      		push	Acc
                 		;//////////////////////////////
                 		;// program timer0 to generate 125Hz
                 		;// using 16Mhz crystal and 1024 prescaler
                 		;//////////////////////////////
003675 e08f      		ldi	Acc,(1<<WGM01)|(0<<WGM00)|_TIMERM0_DIV_1024	;timer0 in CTC mode prescaler 1024
003676 bf83      		out	TCCR0,Acc
003677 e78c      		ldi	Acc,_RTC_TIMER0_DIV_FACTOR			;load CTC with division factor to generate
                 									;125 Hz
003678 bf81      		out	OCR0,Acc
003679 930f
00367a b70f
00367b 94f8
00367c e982
00367d e396
00367e 9380 011c
003680 9390 011d
003682 bf0f
003683 910f      		_SET_HANDDLE	_HDC_OC0_VECT,_RTC_PROCESS		;set handdle interrupt process
003684 b787      		in	Acc,TIMSK					;enable TIMER0 CTC interrupt
003685 6082      		ori	Acc,(1<<OCIE0)
003686 bf87      		out	TIMSK,Acc
003687 e080      		ldi	Acc,0
003688 9380 0ec8 		sts	_RTC_DIV,Acc
00368a 9380 0ecc 		sts	_RTC_TICKS+0,Acc
00368c 9380 0ecd 		sts	_RTC_TICKS+1,Acc
00368e 918f      		pop	Acc						;restore SREG state
00368f bf8f      		out	SREG,Acc
003690 918f      		pop	Acc						;restore useds
003691 9508      		ret
                 
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : RTC						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : RTC_PROCESS					|
                 	;>--------------------------------------------------------------<
                 	;| Description: Process RTC engine				|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	None						|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	Interrupts are enabled when enter here		|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_RTC_PROCESS:
003692 938f      		push	Acc						;Save Acc
003693 b78f      		in	Acc,SREG					;Save SREG
003694 938f      		push	Acc
003695 9478      		sei							;enable interrupts
                 		;/////////////////////////
                 		;// PUMP RTC TICK
                 		;/////////////////////////
003696 9180 0ecc 		lds	Acc,_RTC_TICKS+0
003698 5f8f      		subi	Acc,-1
003699 9380 0ecc 		sts	_RTC_TICKS+0,Acc
00369b 9180 0ecd 		lds	Acc,_RTC_TICKS+1
00369d 4f8f      		sbci	Acc,-1
00369e 9380 0ecd 		sts	_RTC_TICKS+1,Acc
                 		;/////////////////////////
                 		;// PUMP RTC DIV
                 		;/////////////////////////
0036a0 9180 0ec8 		lds	Acc,_RTC_DIV					;GET_DIV
0036a2 9583      		inc	Acc						;DIV++
0036a3 9380 0ec8 		sts	_RTC_DIV,Acc		
0036a5 378d      		cpi	Acc,_RTC_SECOND_DIV_FACTOR			;DIV>=FACTOR?
0036a6 f108      		brlo	_RTC_PROCESS_EXIT				;no, branch
0036a7 e080      		ldi	Acc,0						;else clear DIV
0036a8 9380 0ec8 		sts	_RTC_DIV,Acc		
                 		;/////////////////////////
                 		;// PUMP RTC SECOND
                 		;/////////////////////////		
0036aa 9180 0ec9 		lds	Acc,_RTC_SECOND					;get second
0036ac 9583      		inc	Acc						;second++
0036ad 9380 0ec9 		sts	_RTC_SECOND,Acc
0036af 338c      		cpi	Acc,60						;second>60?
0036b0 f0b8      		brlo	_RTC_PROCESS_EXIT				;no, branch
0036b1 2788      		clr	Acc						;else second=0
0036b2 9380 0ec9 		sts	_RTC_SECOND,Acc
                 		;/////////////////////////
                 		;// PUMP RTC MINUTE
                 		;/////////////////////////		
0036b4 9180 0eca 		lds	Acc,_RTC_MINUTE					;get minute
0036b6 9583      		inc	Acc						;minute++
0036b7 9380 0eca 		sts	_RTC_MINUTE,Acc
0036b9 338c      		cpi	Acc,60						;minute>60?
0036ba f068      		brlo	_RTC_PROCESS_EXIT				;no, branch
0036bb 2788      		clr	Acc						;else second=0
0036bc 9380 0eca 		sts	_RTC_MINUTE,Acc
                 		;/////////////////////////
                 		;// PUMP RTC HOUR
                 		;/////////////////////////		
0036be 9180 0ecb 		lds	Acc,_RTC_HOUR					;get hour
0036c0 9583      		inc	Acc						;hour++
0036c1 9380 0ecb 		sts	_RTC_HOUR,Acc
0036c3 3188      		cpi	Acc,24						;hour>24?
0036c4 f018      		brlo	_RTC_PROCESS_EXIT				;no, branch
0036c5 2788      		clr	Acc						;else second=0
0036c6 9380 0ecb 		sts	_RTC_HOUR,Acc
                 	_RTC_PROCESS_EXIT:
                 		;/////////////////////////
                 		;// PROCESS RELAY EVENTS
                 		;/////////////////////////		
0036c8 940e 1395 		call	_MOTION_RELAY_PROCESS_EVENT			;process relay events
0036ca 918f      		pop	Acc
0036cb bf8f      		out	SREG,Acc					;restore Acc
0036cc 918f      		pop	Acc
0036cd 9508      		ret
                 
