                 	_RASTER_ADJUST_COLOR_MASK_20:
000542 7083      		andi	Acc,0x3					;clear bits 2..7
000543 fd80      		sbrc	Acc,0					;skip if bit 0=0
000544 6584      		ori	Acc,0b01010100				;else set bits 2,4,6
000545 fd81      		sbrc	Acc,1					;skip if bit 1=0
000546 6a88      		ori	Acc,0b10101000				;else set bits 3,5,7
000547 c005      		rjmp	_RASTER_ADJUST_COLOR_MASK_40		;process the rest
                 		;////////////////////////////////
                 		;// ADJUST COLOR FOR 4 BIT
                 		;////////////////////////////////
