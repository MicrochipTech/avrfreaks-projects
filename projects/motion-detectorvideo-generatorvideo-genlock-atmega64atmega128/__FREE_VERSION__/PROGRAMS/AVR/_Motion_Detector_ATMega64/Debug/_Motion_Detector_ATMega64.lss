002290 958a      		dec	Acc						;			;1
002291 f7f1      		brne	PC-1						;			;2/1
002292 9181      		ld	Acc,Z+									;2
002293 0000      		nop										;1
002294 0000      		nop										;1
002295 0000      		nop										;1
002296 0000      		nop										;1
002297 708f      		andi	Acc,0x0f					;extract high pixel	;1
002298 5f83      		subi	Acc,-_VIDEO_OUT_LEVEL_BLACK			;add black level	;1
002299 9380 0062 		sts	_VIDEO_OUT_PORT_OUTPUT,Acc			;put on screen		;2
00229b 950a      		dec	AccT						;			;1
00229c f731      		brne	_VIDEO_OUT_VIDEO_16LD_ODD_10			;until all done		;2/1
00229d 70c7      		andi	YL,7						;mask low bits		
