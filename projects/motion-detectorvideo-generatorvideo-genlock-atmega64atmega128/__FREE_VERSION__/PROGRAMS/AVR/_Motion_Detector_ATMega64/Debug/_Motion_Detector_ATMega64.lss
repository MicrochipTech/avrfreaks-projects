000548 2f98      		mov	AccH,Acc				;copy bits 0.3-->4..7
000549 9592      		swap	AccH
00054a 7f90      		andi	AccH,0xf0
00054b 708f      		andi	Acc,0x0f
00054c 2b89      		or	Acc,AccH
                 	_RASTER_ADJUST_COLOR_MASK_40:
00054d 919f      		pop	AccH
00054e 9508      		ret
