                 ;+----------------------------------------------------------------------+
                 ;| _DUMPHEX								|
                 ;| Convert unsigned byte to hex string ASCII with fixed size 2 into SRAM|
                 ;|								  	|
                 ;| Input  : Acc unsigned byte input				  	|
                 ;|	    Z--> output string hex inc ASCII				|
                 ;| Output : Z--> next sram after 2nd hex digit 			  	|
                 ;+----------------------------------------------------------------------+
                 	_DUMPHEX:
000156 938f      		push	Acc					;save acc
000157 938f      		push	Acc
000158 9582      		swap 	Acc
000159 d004      		rcall	_DUMPHEX00				;convert 1s nibble
00015a 918f      		pop	Acc
00015b d002      		rcall	_DUMPHEX00				;convert 2s nibble
00015c 918f      		pop	Acc
00015d 9508      		ret
                 	_DUMPHEX00:
00015e 708f      		andi	Acc,0x0f				;only low nibble 
00015f 6380      		ori	Acc,0x30				;convert to ASCII
000160 338a      		cpi	Acc,0x3a				;digits ?
000161 f008      		brlo	_DUMPHEX10				;yes, branch
000162 5f89      		subi	Acc,-7					;convert to A..F
                 	_DUMPHEX10:
000163 9381      		st	Z+,Acc
000164 9508      		ret
                 ;+----------------------------------------------------------------------+
                 ;| _TOHEX2								|
                 ;| Convert unsigned byte to hex string ASCII into SRAM zero ended	|
                 ;|								  	|
                 ;| Input  : Acc unsigned byte input				  	|
                 ;|	    								|
                 ;| Output : Z--> Hex 2 digits into SRAM zero ended		  	|
                 ;+----------------------------------------------------------------------+
                 	_TOHEX2:
000165 938f      		push	Acc
000166 e4eb
000167 e0f1      		ldiw	Z,_DF_STR				;Z-->string output area
000168 dfed      		rcall	_DUMPHEX				;convert to hex 2 digits
000169 e080      		ldi	Acc,0
00016a 8380      		st	Z,Acc					;put Zero ended
00016b e4eb
00016c e0f1      		ldiw	Z,_DF_STR				;Z-->string output area
00016d 918f      		pop	Acc
00016e 9508      		ret		
