                 ;>--------------------------------------------------------------<
                 ;| OBJECT     : CPULED						|
                 ;>--------------------------------------------------------------<
                 ;| DESCRIPTION: This object set state of cpu led		|
                 ;>--------------------------------------------------------------<
                 
                 ;>----------------------<
                 ;| OBJECT DEFINITIONS	|
                 ;>----------------------<
                 
                 ;-------------------------< PORT DEFINITONS >----------------------------
                 
                 .EQU	_CPULED_PORT_OUTPUT	= PORTG
                 .EQU	_CPULED_PORT_DIR	= DDRG
                 .EQU	_CPULED_PORT_INPUT	= PING
                 
                 
                 ;-------------------------< CONSTANT DEFINITONS >------------------------
                 
                 .EQU	_CPULED_LED_BIT		= 2
                 
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : CPULED						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : Initialize Object				|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	None						|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	Disable interrupts during initialization	|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_CPULED_INIT:
0012fb 938f      		push	Acc						;save useds
0012fc b78f      		in	Acc,SREG					;save SREG
0012fd 94f8      		cli							;disable interrupts during initialization
0012fe 938f      		push	Acc
0012ff 9180 0065 		lds	Acc,PORTG					;get old state
001301 7f8b      		andi	Acc,~(1<<_CPULED_LED_BIT)			;set led enable
001302 9380 0065 		sts	PORTG,Acc
001304 9180 0064 		lds	Acc,DDRG					;get old state
001306 6084      		ori	Acc,(1<<_CPULED_LED_BIT)			;set as output
001307 9380 0064 		sts	DDRG,Acc
001309 918f      		pop	Acc						;restore SREG state
00130a bf8f      		out	SREG,Acc
00130b 918f      		pop	Acc						;restore useds
00130c 9508      		ret
                 
                 	;>--------------------------------------------------------------<
                 	;| OBJECT     : CPULED						|
                 	;>--------------------------------------------------------------<
                 	;| FUNCTION   : Altenate CPU LED state 				|
                 	;>--------------------------------------------------------------<
                 	;| INPUT      :	None						|
                 	;| OUTPUT     :	None						|
                 	;| DESTROYED  :	None						|
                 	;| TIME	      :	----						|
                 	;>--------------------------------------------------------------<
                 	;| OBSERVATION:	----						|
                 	;+--------------------------------------------------------------+-------------------------------------------------------------------------------\
                 	;|																		|
                 	_CPULED_ALTERNATE:
00130d 938f      		push	Acc						;save useds
00130e b78f      		in	Acc,SREG					;get SREG
00130f 94f8      		cli							;disable interrupts
001310 938f
001311 939f      		pushaw
001312 9180 0065 		lds	Acc,_CPULED_PORT_OUTPUT				;get led state
001314 e094      		ldi	AccH,(1<<_CPULED_LED_BIT)			;eor mask
001315 2789      		eor	Acc,AccH
001316 9380 0065 		sts	_CPULED_PORT_OUTPUT,Acc				;altered
001318 919f
001319 918f      		popaw				
00131a bf8f      		out	SREG,Acc					;restore SREG
00131b 918f      		pop	Acc
00131c 9508      		ret
                 
