000534 939f      		push	AccH
                 		;/////////////////////////////////
                 		;// ADJUST COLOR INPUT
                 		;/////////////////////////////////
000535 9190 0166 		lds	AccH,_RASTER_BIT_SIZE			;get bit size
000537 3091      		cpi	AccH,1					;1 bit ?
000538 f029      		breq	_RASTER_ADJUST_COLOR_MASK_10		;yes, branch
000539 3092      		cpi	AccH,2					;2 bit ?
00053a f039      		breq	_RASTER_ADJUST_COLOR_MASK_20		;yes, branch
00053b 3094      		cpi	AccH,4					;4 bit ?
