                 		;| set VIDEO SYNC for input with pull up
                 		;+--------------------------------------
0026f9 9ad9      		sbi	_VIDEO_SYNC_PORT_OUTPUT,_VIDEO_SYNC_FIELD_BIT	;all pullup and input
0026fa 9ada      		sbi	_VIDEO_SYNC_PORT_OUTPUT,_VIDEO_SYNC_COMPOSITE_BIT
0026fb 9adb      		sbi	_VIDEO_SYNC_PORT_OUTPUT,_VIDEO_SYNC_VERTICAL_BIT
                 		
0026fc 98d1      		cbi	_VIDEO_SYNC_PORT_DIR,_VIDEO_SYNC_FIELD_BIT
0026fd 98d2      		cbi	_VIDEO_SYNC_PORT_DIR,_VIDEO_SYNC_COMPOSITE_BIT
0026fe 98d3      		cbi	_VIDEO_SYNC_PORT_DIR,_VIDEO_SYNC_VERTICAL_BIT
0026ff 9ad7      		sbi	_VIDEO_SYNC_PORT_DIR,7
                 		;+--------------------------------------
                 		;| set comparator input with pull up
                 		;+--------------------------------------
002700 981b      		cbi	_VIDEO_COMP_PORT_OUTPUT,_VIDEO_COMP_R2R_BIT	;comparator pins as high impedance
002701 981a      		cbi	_VIDEO_COMP_PORT_OUTPUT,_VIDEO_COMP_VIDEO_BIT
002702 981c      		cbi	_VIDEO_COMP_PORT_OUTPUT,_VIDEO_COMP_PWM_B_BIT	;start pwm low level
002703 9813      		cbi	_VIDEO_COMP_PORT_DIR,_VIDEO_COMP_R2R_BIT	;comparator pins as input
002704 9812      		cbi	_VIDEO_COMP_PORT_DIR,_VIDEO_COMP_VIDEO_BIT
002705 9a14      		sbi	_VIDEO_COMP_PORT_DIR,_VIDEO_COMP_PWM_B_BIT	;pwm as output
                 		;+--------------------------------------
                 		;| program TIMER3 to generate 7812,5Hz of 10bit PWM
                 		;| MODE 3 WGM33=0 WGM32=0 WGM31=1 WGM30=1
                 		;| PRESCALER 1X
                 		;+--------------------------------------
002706 e283      		ldi	Acc,(1<<WGM31)|(1<<WGM30)|(1<<COM3B1)|(0<<COM3B0);mode 3 (NORMAL FASE CORRECT PWM 10 BITS)
002707 9380 008b 		sts	TCCR3A,Acc
002709 e081      		ldi	Acc,(0<<WGM33)|(0<<WGM32)|_TIMERM13_DIV_1	;prescaler = 1x
00270a 9380 008a 		sts	TCCR3B,Acc
00270c eb84
00270d e090      		ldiaw	180						;force 50% of pwm at start
00270e 9390 0085 		sts	OCR3BH,AccH
002710 9380 0084 		sts	OCR3BL,Acc
