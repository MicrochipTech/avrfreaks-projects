00229e 30c6      		cpi	YL,6						;line to change
00229f f429      		brne	_VIDEO_OUT_VIDEO_16LD_ODD_20			;no, branch
0022a0 93e0 018f
0022a2 93f0 0190 		stsw	_VIDEO_OUT_PIXEL_POINTER,Z			;dummy load to obtain correct
0022a4 c005      		rjmp	_VIDEO_OUT_VIDEO_16LD_ODD_EXIT
