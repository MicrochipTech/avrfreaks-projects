                 	;+----------------------------------------------------------------------------------------------+
                 	;| _EEPROM_WRITE	WRITE DATA INTO EEPROM							|
                 	;|			SAMPLE USING EEPROM_WRITE TO WRITE DATA 0X33 INTO ADDRESS 0X14		|
                 	;|												|
                 	;|			LDI	ACC,0X33							|
                 	;|			LDI	ACCH,LOW(0X14)							|
                 	;|			LDI	ACCT,HIGH(0X14)							|
                 	;|			call 	EEPROM_WRITE							|
                 	;|----------------------------------------------------------------------------------------------|												|
                 	;| _EEPROM_READ		READ DATA FROM EEPROM							|
                 	;|			SAMPLE USING EEPROM_READ TO READ A DATA FROM A ADDRESS 0X14		|
                 	;|												|
                 	;|			LDI	ACCH,LOW(0X14)							|
                 	;|			LDI	ACCT,HIGH(0X14)							|
                 	;|			call 	EEPROM_READ	;AFTER THIS ACC CONTENTS HAVE A DATA READ	|
                 	;+----------------------------------------------------------------------------------------------+
                 	;+--------------------------+
                 	;| WRITE DATA INTO EEPROM   |
                 	;|			    |
                 	;| input: Acc  Data	    |
                 	;| 	  AccH Address low  |
                 	;|	  AccT Address High |
                 	;+--------------------------+
                 	_EEPROM_WRITE:
00123c 99e1      		sbic	EECR,EEWE			;wait last write operation is done
00123d 940c 123c 		jmp 	_EEPROM_WRITE			;wait
00123f bb0f      		out	EEARH,AccT			;set address High
001240 bb9e      		out	EEARL,AccH			;set address Low
001241 bb8d      		out	EEDR,Acc			;set data
001242 938f      		push	Acc				;Save Acc
001243 b78f      		in	Acc,SREG			;get SREG state
001244 938f      		push	Acc				;Save Acc Again
001245 94f8      		cli
001246 9ae2      		sbi	EECR,EEMWE  			;request eeprom write
001247 9ae1      		sbi	EECR,EEWE			;place write command
001248 918f      		pop	Acc				;restore SREG 
001249 bf8f      		out 	SREG,Acc
00124a 918f      		pop	Acc				;restore Acc
00124b 9508      		ret	
                 	;+--------------------------+
                 	;| READ DATA FROM EEPROM    |
                 	;|			    |
                 	;| input: AccH Address low  |
                 	;|	  AccT Address High | 	
                 	;| output: Acc data	    |
                 	;+--------------------------+
                 	_EEPROM_READ:
