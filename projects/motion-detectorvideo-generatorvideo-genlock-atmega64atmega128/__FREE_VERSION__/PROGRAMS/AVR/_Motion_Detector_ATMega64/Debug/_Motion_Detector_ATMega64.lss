00271a 9380 0065 		sts	_VIDEO_MIX_PORT_OUTPUT,Acc
00271c 9180 0064 		lds	Acc,_VIDEO_MIX_PORT_DIR				;set both bits as output
00271e 6088      		ori	Acc,(1<<_VIDEO_MIX_DIRECTION_BIT)
00271f 6180      		ori	Acc,(1<<_VIDEO_MIX_ENABLE_BIT)
002720 9380 0064 		sts	_VIDEO_MIX_PORT_DIR,Acc
