_0x45:
;     415                         lcd_putchar(key1);
	ST   -Y,R16
	RCALL _lcd_putchar
;     416                         i1=key1;
	MOV  R19,R16
;     417                         key1=0;
	LDI  R16,LOW(0)
;     418                         while (key1==0)
_0x46:
	CPI  R16,0
	BRNE _0x48
;     419                         {         
;     420                         key1=getchar_kb();
	RCALL _getchar_kb
	MOV  R16,R30
;     421                         }
	RJMP _0x46
_0x48:
;     422                         lcd_putchar(key1);
	ST   -Y,R16
	RCALL _lcd_putchar
;     423                         i2=key1;
	MOV  R20,R16
;     424                         lcd_clear();
	RCALL _lcd_clear
;     425                         lcd_putchar(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _lcd_putchar
;     426                         lcd_putsf(" YOU HAVE ENTERED");
	__POINTW1FN _0,80
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     427                         lcd_gotoxy(0,1);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;     428                         lcd_putchar(1);
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL _lcd_putchar
;     429                         lcd_putsf(" '0x");
	__POINTW1FN _0,98
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     430                         lcd_putchar(i1);
	ST   -Y,R19
	RCALL _lcd_putchar
;     431                         lcd_putchar(i2);
	ST   -Y,R20
	RCALL _lcd_putchar
;     432                         lcd_putsf("' PRESS ENTER");
	__POINTW1FN _0,103
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     433                         lcd_gotoxy(0,2);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(2)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;     434                         lcd_putchar(2);
