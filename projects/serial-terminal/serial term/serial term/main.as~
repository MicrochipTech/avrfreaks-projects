	ST   -Y,R30
	RCALL _lcd_putchar
;     438                         lcd_putsf(" OR F5 TO RE-ENTER");
	__POINTW1FN _0,135
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     439                         _lcd_ready();
	RCALL __lcd_ready
;     440                         _lcd_write_data(0xc);
	LDI  R30,LOW(12)
	ST   -Y,R30
	RCALL __lcd_write_data
;     441                         while (key1!=0xfd)
_0x49:
	CPI  R16,253
	BREQ _0x4B
;     442                         {
;     443                         if(key1==0x0d)
	CPI  R16,13
	BRNE _0x4C
;     444                         {       
;     445                                 h1=i1-0x30;
	MOV  R30,R19
	SUBI R30,LOW(48)
	MOV  R21,R30
;     446                                 h2=i2-0x30;
	MOV  R30,R20
	SUBI R30,LOW(48)
	STD  Y+7,R30
;     447                                 i1=h1*16;
	MOV  R30,R21
	SWAP R30
	ANDI R30,0xF0
	MOV  R19,R30
;     448                                 h1=i1+h2; 
	LDD  R30,Y+7
	ADD  R30,R19
	MOV  R21,R30
;     449                                 UBRR=0x17;
	LDI  R30,LOW(23)
	OUT  0x9,R30
;     450                                 loop=1;
	LDI  R30,LOW(1)
	STD  Y+6,R30
;     451                                 lcd_clear();
	RCALL _lcd_clear
;     452                                 i=0;
	LDI  R18,LOW(0)
;     453                                 return;
	RJMP _0x15A
;     454                         }         
;     455                         key1=getchar_kb();
_0x4C:
	RCALL _getchar_kb
	MOV  R16,R30
;     456                         }
	RJMP _0x49
_0x4B:
;     457                         
;     458                         }
	RJMP _0x40
_0x42:
;     459                         
;     460                         
;     461                 }
;     462                     
;     463         }
_0x3F:
;     464         
;     465         
;     466         
;     467         
;     468         
;     469 } 
_0x15A:
	RCALL __LOADLOCR6
	ADIW R28,8
	RET
;     470 
;     471 #include "gpr.h"
;     472 
;     473 void print_hexbyte(unsigned char i)
;     474 {
_print_hexbyte:
;     475     unsigned char h, l;
;     476     
;     477     h = i & 0xF0;               // High nibble
	RCALL __SAVELOCR2
;	i -> Y+2
;	h -> R16
;	l -> R17
	LDD  R30,Y+2
	ANDI R30,LOW(0xF0)
	MOV  R16,R30
;     478     h = h>>4;
	SWAP R16
	ANDI R16,0xF
;     479     h = h + '0';
	SUBI R16,-LOW(48)
;     480     
;     481     if (h > '9')
	LDI  R30,LOW(57)
	CP   R30,R16
	BRSH _0x4D
;     482         h = h + 7;
	SUBI R16,-LOW(7)
;     483 
;     484     l = (i & 0x0F)+'0';         // Low nibble
_0x4D:
	LDD  R30,Y+2
	ANDI R30,LOW(0xF)
	SUBI R30,-LOW(48)
	MOV  R17,R30
;     485     if (l > '9')
	LDI  R30,LOW(57)
	CP   R30,R17
	BRSH _0x4E
;     486         l = l + 7;
	SUBI R17,-LOW(7)
;     487 
;     488 
;     489     putchar(h);
_0x4E:
	ST   -Y,R16
	RCALL _putchar
;     490     putchar(l);
	ST   -Y,R17
	RCALL _putchar
;     491 }
	RCALL __LOADLOCR2
	ADIW R28,3
	RET
;     492 
;     493 
;     494 //void delay(char d)
;     495 //{
;     496 //    char i,j,k;
;     497 //    for(i=0; i<d; i++)
;     498 //        for(j=0; j<40; j++)
;     499 //            for(k=0; k<176; k++);
;     500 //}
;     501 
;     502 //#include <pgmspace.h>
;     503 #include "kb.h"
;     504 //#include "serial.h"
;     505 #include "gpr.h"
;     506 
;     507 #include "scancodes.h"
;     508 
;     509 #define BUFF_SIZE 64
;     510 
;     511 unsigned char edge, bitcount;                // 0 = neg.  1 = pos.
;     512 
;     513 unsigned char kb_buffer[BUFF_SIZE];

	.DSEG
_kb_buffer:
	.BYTE 0x40
;     514 unsigned char *inpt, *outpt;
_outpt:
	.BYTE 0x2
;     515 unsigned char buffcnt;
;     516 
;     517 
;     518 void init_kb(void)
;     519 {

	.CSEG
_init_kb:
;     520     inpt =  kb_buffer;                        // Initialize buffer
	LDI  R30,LOW(_kb_buffer)
	LDI  R31,HIGH(_kb_buffer)
	__PUTW1R 12,13
;     521     outpt = kb_buffer;
	LDI  R30,LOW(_kb_buffer)
	LDI  R31,HIGH(_kb_buffer)
	STS  _outpt,R30
	STS  _outpt+1,R31
;     522     buffcnt = 0;
	CLR  R14
;     523 
;     524     MCUCR = 2;                                // INT0 interrupt on falling edge
	LDI  R30,LOW(2)
	OUT  0x35,R30
;     525     edge = 0;                                // 0 = falling edge  1 = rising edge
	CLR  R10
;     526     bitcount = 11;
	LDI  R30,LOW(11)
	MOV  R11,R30
;     527 }
	RET
;     528 
;     529 interrupt [EXT_INT0] void ext_int0_isr(void)
;     530 {
_ext_int0_isr:
	ST   -Y,R0
	ST   -Y,R1
	ST   -Y,R15
	ST   -Y,R22
	ST   -Y,R23
	ST   -Y,R24
	ST   -Y,R25
	ST   -Y,R26
	ST   -Y,R27
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
;     531     static unsigned char data;                // Holds the received scan code

	.DSEG
_data_S9:
	.BYTE 0x1

	.CSEG
;     532 
;     533     if (!edge)                                // Routine entered at falling edge
	TST  R10
	BRNE _0x4F
;     534     {
;     535         if(bitcount < 11 && bitcount > 2)    // Bit 3 to 10 is data. Parity bit,
	LDI  R30,LOW(11)
	CP   R11,R30
	BRSH _0x51
	LDI  R30,LOW(2)
	CP   R30,R11
	BRLO _0x52
_0x51:
	RJMP _0x50
_0x52:
;     536         {                                    // start and stop bits are ignored.
;     537             data = (data >> 1);
	LDS  R30,_data_S9
	LSR  R30
	STS  _data_S9,R30
;     538             if(PIND & 8)
	SBIS 0x10,3
	RJMP _0x53
;     539                 data = data | 0x80;            // Store a '1'
	LDS  R30,_data_S9
	ORI  R30,0x80
	STS  _data_S9,R30
;     540         }
_0x53:
;     541 
;     542         MCUCR = 3;                            // Set interrupt on rising edge
_0x50:
	LDI  R30,LOW(3)
	OUT  0x35,R30
;     543         edge = 1;
	LDI  R30,LOW(1)
	MOV  R10,R30
;     544         
;     545     } else {                                // Routine entered at rising edge
	RJMP _0x54
_0x4F:
;     546 
;     547         MCUCR = 2;                            // Set interrupt on falling edge
	LDI  R30,LOW(2)
	OUT  0x35,R30
;     548         edge = 0;
	CLR  R10
;     549 
;     550         if(--bitcount == 0)                    // All bits received
	DEC  R11
	BRNE _0x55
;     551         {
;     552             decode(data);
	LDS  R30,_data_S9
	ST   -Y,R30
	RCALL _decode
;     553             bitcount = 11;
	LDI  R30,LOW(11)
	MOV  R11,R30
;     554         }
;     555     }
_0x55:
_0x54:
;     556 }
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R27,Y+
	LD   R26,Y+
	LD   R25,Y+
	LD   R24,Y+
	LD   R23,Y+
	LD   R22,Y+
	LD   R15,Y+
	LD   R1,Y+
	LD   R0,Y+
	RETI
;     557 
;     558 
;     559 void decode(unsigned char sc)
;     560 {
_decode:
;     561     static unsigned char is_up=0, shift = 0, mode = 0, scrollup = 0,ctrl =0;

	.DSEG
_is_up_SA:
	.BYTE 0x1
_shift_SA:
	.BYTE 0x1
_mode_SA:
	.BYTE 0x1
_scrollup_SA:
	.BYTE 0x1
_ctrl_SA:
	.BYTE 0x1

	.CSEG
;     562     unsigned char i;
;     563 
;     564     if (!is_up)                // Last data received was the up-key identifier
	ST   -Y,R16
;	sc -> Y+1
;	i -> R16
	LDS  R30,_is_up_SA
	CPI  R30,0
	BREQ PC+2
	RJMP _0x56
;     565     {
;     566         switch (sc)
	LDD  R30,Y+1
;     567         {
;     568               
;     569                 
;     570           case 0xF0 :        // The up-key identifier
	CPI  R30,LOW(0xF0)
	BRNE _0x5A
;     571             is_up = 1;
	LDI  R30,LOW(1)
	STS  _is_up_SA,R30
;     572             break;
	RJMP _0x59
;     573                    
;     574           case 0x14 :       //ctrl
_0x5A:
	CPI  R30,LOW(0x14)
	BRNE _0x5B
;     575             ctrl = 1;
	LDI  R30,LOW(1)
	STS  _ctrl_SA,R30
;     576             break;
	RJMP _0x59
;     577             
;     578           case 0x12 :        // Left SHIFT
_0x5B:
	CPI  R30,LOW(0x12)
	BRNE _0x5C
;     579             shift = 1;
	LDI  R30,LOW(1)
	STS  _shift_SA,R30
;     580             break;
	RJMP _0x59
;     581 
;     582           case 0x59 :        // Right SHIFT
_0x5C:
	CPI  R30,LOW(0x59)
	BRNE _0x5D
;     583             shift = 1;
	LDI  R30,LOW(1)
	STS  _shift_SA,R30
;     584             break;
	RJMP _0x59
;     585 
;     586           case 0x05 :        // F1
_0x5D:
	CPI  R30,LOW(0x5)
	BRNE _0x61
;     587             if(mode == 0)
	LDS  R30,_mode_SA
	CPI  R30,0
	BRNE _0x5F
;     588                 mode = 1;    // Enter scan code mode
	LDI  R30,LOW(1)
	STS  _mode_SA,R30
;     589             if(mode == 2)
_0x5F:
	LDS  R26,_mode_SA
	CPI  R26,LOW(0x2)
	BRNE _0x60
;     590                 mode = 3;    // Leave scan code mode
	LDI  R30,LOW(3)
	STS  _mode_SA,R30
;     591             break;
_0x60:
	RJMP _0x59
;     592 
;     593           default:
_0x61:
;     594             if(mode == 0 || mode == 3)        // If ASCII mode
	LDS  R26,_mode_SA
	CPI  R26,LOW(0x0)
	BREQ _0x63
	CPI  R26,LOW(0x3)
	BREQ _0x63
	RJMP _0x62
_0x63:
;     595             {  
;     596                 if(ctrl == 1)
	LDS  R26,_ctrl_SA
	CPI  R26,LOW(0x1)
	BRNE _0x65
;     597                 {   
;     598                     if(sc == 0x21)
	LDD  R26,Y+1
	CPI  R26,LOW(0x21)
	BRNE _0x66
;     599                     putchar(0x03);
	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _putchar
;     600                 }
_0x66:
;     601                 if(!shift )                    // If shift not pressed,
_0x65:
	LDS  R30,_shift_SA
	CPI  R30,0
	BREQ PC+2
	RJMP _0x67
;     602                 {                            // do a table look-up
;     603                     for(i = 0; unshifted[i][0]!=sc && unshifted[i][0]; i++);
	LDI  R16,LOW(0)
_0x69:
	LDI  R26,LOW(_unshifted*2)
	LDI  R27,HIGH(_unshifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	MOV  R26,R30
	LDD  R30,Y+1
	CP   R30,R26
	BREQ _0x6B
	LDI  R26,LOW(_unshifted*2)
	LDI  R27,HIGH(_unshifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	CPI  R30,0
	BRNE _0x6C
_0x6B:
	RJMP _0x6A
_0x6C:
	SUBI R16,-1
	RJMP _0x69
_0x6A:
;     604                     if (unshifted[i][0] == sc) 
	LDI  R26,LOW(_unshifted*2)
	LDI  R27,HIGH(_unshifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	MOV  R26,R30
	LDD  R30,Y+1
	CP   R30,R26
	BRNE _0x6D
;     605                     {
;     606                         put_kbbuff(unshifted[i][1]);
	LDI  R26,LOW(_unshifted*2)
	LDI  R27,HIGH(_unshifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	ADIW R30,1
	LPM
	MOV  R30,R0
	ST   -Y,R30
	RCALL _put_kbbuff
;     607                     }
;     608                 } 
_0x6D:
;     609                 else 
	RJMP _0x6E
_0x67:
;     610                 {   
;     611                     
;     612                                     // If shift pressed
;     613                         for(i = 0; shifted[i][0]!=sc && shifted[i][0]; i++);
	LDI  R16,LOW(0)
_0x70:
	LDI  R26,LOW(_shifted*2)
	LDI  R27,HIGH(_shifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	MOV  R26,R30
	LDD  R30,Y+1
	CP   R30,R26
	BREQ _0x72
	LDI  R26,LOW(_shifted*2)
	LDI  R27,HIGH(_shifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	CPI  R30,0
	BRNE _0x73
_0x72:
	RJMP _0x71
_0x73:
	SUBI R16,-1
	RJMP _0x70
_0x71:
;     614                         if (shifted[i][0] == sc) 
	LDI  R26,LOW(_shifted*2)
	LDI  R27,HIGH(_shifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	LPM
	MOV  R30,R0
	MOV  R26,R30
	LDD  R30,Y+1
	CP   R30,R26
	BRNE _0x74
;     615                             {
;     616                                 put_kbbuff(shifted[i][1]);
	LDI  R26,LOW(_shifted*2)
	LDI  R27,HIGH(_shifted*2)
	MOV  R30,R16
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	ADIW R30,1
	LPM
	MOV  R30,R0
	ST   -Y,R30
	RCALL _put_kbbuff
;     617                             }
;     618 
;     619                     
;     620                     
;     621                 }
_0x74:
_0x6E:
;     622             } else{                            // Scan code mode
	RJMP _0x75
_0x62:
;     623                 print_hexbyte(sc);            // Print scan code
	LDD  R30,Y+1
	ST   -Y,R30
	RCALL _print_hexbyte
;     624                 put_kbbuff(' ');
	LDI  R30,LOW(32)
	ST   -Y,R30
	RCALL _put_kbbuff
;     625                 put_kbbuff(' ');
	LDI  R30,LOW(32)
	ST   -Y,R30
	RCALL _put_kbbuff
;     626             }
_0x75:
;     627             break;
;     628         }
_0x59:
;     629     } else {
	RJMP _0x76
_0x56:
;     630         is_up = 0;
	LDI  R30,LOW(0)
	STS  _is_up_SA,R30
;     631                                     // Two 0xF0 in a row not allowed
;     632         switch (sc)
	LDD  R30,Y+1
;     633         {
;     634         case 0x14:              //left  ctrl
	CPI  R30,LOW(0x14)
	BRNE _0x7A
;     635         ctrl = 0;
	LDI  R30,LOW(0)
	STS  _ctrl_SA,R30
;     636         break;
	RJMP _0x79
;     637         
;     638           case 0x12 :                        // Left SHIFT
_0x7A:
	CPI  R30,LOW(0x12)
	BRNE _0x7B
;     639             shift = 0;
	LDI  R30,LOW(0)
	STS  _shift_SA,R30
;     640             break;
	RJMP _0x79
;     641             
;     642           case 0x59 :                        // Right SHIFT
_0x7B:
	CPI  R30,LOW(0x59)
	BRNE _0x7C
;     643             shift = 0;
	LDI  R30,LOW(0)
	STS  _shift_SA,R30
;     644             break;
	RJMP _0x79
;     645 
;     646           case 0x05 :                        // F1
_0x7C:
	CPI  R30,LOW(0x5)
	BRNE _0x7D
;     647             if(mode == 1)
	LDS  R26,_mode_SA
	CPI  R26,LOW(0x1)
	BRNE _0x7E
;     648                 mode = 2;
	LDI  R30,LOW(2)
	STS  _mode_SA,R30
;     649             if(mode == 3)
_0x7E:
	LDS  R26,_mode_SA
	CPI  R26,LOW(0x3)
	BRNE _0x7F
;     650                 mode = 0;
	LDI  R30,LOW(0)
	STS  _mode_SA,R30
;     651             break;
_0x7F:
	RJMP _0x79
;     652           case 0x06 :                        // F2
_0x7D:
	CPI  R30,LOW(0x6)
	BRNE _0x80
;     653             put_kbbuff(0xff);
	LDI  R30,LOW(255)
	ST   -Y,R30
	RCALL _put_kbbuff
;     654            // printf("scroll up");
;     655            break;
	RJMP _0x79
;     656            case 0x04:
_0x80:
	CPI  R30,LOW(0x4)
	BRNE _0x81
;     657             put_kbbuff(0xfe);
	LDI  R30,LOW(254)
	ST   -Y,R30
	RCALL _put_kbbuff
;     658             break;
	RJMP _0x79
;     659             case 0x03:
_0x81:
	CPI  R30,LOW(0x3)
	BRNE _0x79
;     660             put_kbbuff(0xfd);
	LDI  R30,LOW(253)
	ST   -Y,R30
	RCALL _put_kbbuff
;     661             break;      
;     662         } 
_0x79:
;     663     }    
_0x76:
;     664 } 
	LDD  R16,Y+0
	ADIW R28,2
	RET
;     665 
;     666 void put_kbbuff(unsigned char c)
;     667 {
_put_kbbuff:
;     668     if (buffcnt<BUFF_SIZE)                        // If buffer not full
	LDI  R30,LOW(64)
	CP   R14,R30
	BRSH _0x83
;     669     {
;     670         *inpt = c;                                // Put character into buffer
	LD   R30,Y
	__GETW2R 12,13
	ST   X,R30
;     671         inpt++;                                    // Increment pointer
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	__ADDWRR 12,13,30,31
;     672 
;     673         buffcnt++;
	INC  R14
;     674 
;     675         if (inpt >= kb_buffer + BUFF_SIZE)        // Pointer wrapping
	__POINTW1MN _kb_buffer,64
	CP   R12,R30
	CPC  R13,R31
	BRLO _0x84
;     676             inpt = kb_buffer;
	LDI  R30,LOW(_kb_buffer)
	LDI  R31,HIGH(_kb_buffer)
	__PUTW1R 12,13
;     677     }
_0x84:
;     678 }
_0x83:
_0x159:
	ADIW R28,1
	RET
;     679 
;     680 int getchar_kb(void)
;     681 {
_getchar_kb:
;     682     int byte;  
;     683    
;     684     //while(buffcnt == 0);                        // Wait for data
;     685     if(buffcnt == 0)
	RCALL __SAVELOCR2
;	byte -> R16,R17
	TST  R14
	BRNE _0x85
;     686     {         
;     687     byte=0;
	__GETWRN 16,17,0
;     688     return byte;
	__GETW1R 16,17
	RJMP _0x158
;     689     }
;     690     else
_0x85:
;     691     { 
;     692     byte = *outpt;                                // Get byte
	LDS  R26,_outpt
	LDS  R27,_outpt+1
	LD   R30,X
	LDI  R31,0
	__PUTW1R 16,17
;     693     outpt++;                                    // Increment pointer
	LDS  R30,_outpt
	LDS  R31,_outpt+1
	ADIW R30,1
	STS  _outpt,R30
	STS  _outpt+1,R31
;     694 
;     695     if (outpt >= kb_buffer + BUFF_SIZE)            // Pointer wrapping
	__POINTW1MN _kb_buffer,64
	LDS  R26,_outpt
	LDS  R27,_outpt+1
	CP   R26,R30
	CPC  R27,R31
	BRLO _0x87
;     696         outpt = kb_buffer;
	LDI  R30,LOW(_kb_buffer)
	LDI  R31,HIGH(_kb_buffer)
	STS  _outpt,R30
	STS  _outpt+1,R31
;     697     
;     698     buffcnt--;                                    // Decrement buffer count
_0x87:
	DEC  R14
;     699 
;     700     return byte;
	__GETW1R 16,17
;     701     }
;     702 }
_0x158:
	RCALL __LOADLOCR2P
	RET
;     703 
;     704 
;     705 //#include <ina90.h>
;     706 #include <90s8515.h>
;     707 
;     708 void low_level_init(void)
;     709 {
_low_level_init:
;     710         
;     711         UBRR=0x2f;          // 19200bps @ 4 MHz
	LDI  R30,LOW(47)
	OUT  0x9,R30
;     712     //UCR  = 0x08;        // RX enable, TX enable
;     713     GIMSK= 0x40;        // Enable INT0 interrupt
	LDI  R30,LOW(64)
	OUT  0x3B,R30
;     714     //UCR=0x58; 
;     715     UCR=0xD8; 
	LDI  R30,LOW(216)
	OUT  0xA,R30
;     716 
;     717 #asm("sei")
	sei
;     718     //return 1;
;     719 }
	RET
;     720 
;     721 #include "capture.h"

	.DSEG
_s:
	.BYTE 0x1
_istore:
	.BYTE 0x2
_j:
	.BYTE 0x2
_b:
	.BYTE 0x2
_i:
	.BYTE 0x2
_x:
	.BYTE 0x2
_state:
	.BYTE 0x2
_jstore:
	.BYTE 0x2
_line:
	.BYTE 0x93
_lineflag:
	.BYTE 0x2
;     722 void cap_init (void)
;     723 {

	.CSEG
_cap_init:
;     724 j=0;
	LDI  R30,0
	STS  _j,R30
	STS  _j+1,R30
;     725 i=0;
	LDI  R30,0
	STS  _i,R30
	STS  _i+1,R30
;     726 }
	RET
;     727 void do_states (char s)
;     728 {
_do_states:
;     729 
;     730            
;     731                 //if(up_BN==0)
;     732                 //////printf("it worked");            
;     733               
;     734                   
;     735                 state=bla;
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	STS  _state,R30
	STS  _state+1,R31
;     736                 if(s==0x0a )
	LD   R26,Y
	CPI  R26,LOW(0xA)
	BRNE _0x88
;     737                 state=crlf;
	LDI  R30,0
	STS  _state,R30
	STS  _state+1,R30
;     738                 if(s==0x08)
_0x88:
	LD   R26,Y
	CPI  R26,LOW(0x8)
	BRNE _0x89
;     739                 state=bkspce;
	LDI  R30,LOW(2)
	LDI  R31,HIGH(2)
	STS  _state,R30
	STS  _state+1,R31
;     740                 if(s==0x0d || s==0)
_0x89:
	LD   R26,Y
	CPI  R26,LOW(0xD)
	BREQ _0x8B
	CPI  R26,LOW(0x0)
	BRNE _0x8A
_0x8B:
;     741                 state=ignor;
	LDI  R30,LOW(4)
	LDI  R31,HIGH(4)
	STS  _state,R30
	STS  _state+1,R31
;     742                 if(s==0xff)
_0x8A:
	LD   R26,Y
	CPI  R26,LOW(0xFF)
	BRNE _0x8D
;     743                 state=scrollup;
	LDI  R30,LOW(5)
	LDI  R31,HIGH(5)
	STS  _state,R30
	STS  _state+1,R31
;     744                 if(s==0xfe)
_0x8D:
	LD   R26,Y
	CPI  R26,LOW(0xFE)
	BRNE _0x8E
;     745                 state=scrolldown;
	LDI  R30,LOW(6)
	LDI  R31,HIGH(6)
	STS  _state,R30
	STS  _state+1,R31
;     746                 switch(state)
_0x8E:
	LDS  R30,_state
	LDS  R31,_state+1
;     747                 { 
;     748                       
;     749                         case crlf:
