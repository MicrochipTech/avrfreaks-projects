	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;     406                         lcd_putchar(3);
	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _lcd_putchar
;     407                         lcd_putsf("  UBRR=0x" );
	__POINTW1FN _0,70
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     408                         _lcd_ready();
	RCALL __lcd_ready
;     409                         _lcd_write_data(0xf);
	LDI  R30,LOW(15)
	ST   -Y,R30
	RCALL __lcd_write_data
;     410                         key1=getchar_kb();
	RCALL _getchar_kb
	MOV  R16,R30
;     411                         while (key1==0)
_0x43:
	CPI  R16,0
	BRNE _0x45
;     412                         {         
