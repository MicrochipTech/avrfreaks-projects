	BRGE _0xD7
;    1090                                         {
;    1091                                                  lcd_puts(line[j-3]);      
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x16B
;    1092                                         }
;    1093                                         else
_0xD7:
;    1094                                         {
;    1095                                                  lcd_puts(line[buffersize+j-3]);                                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x16B:
	SBIW R30,3
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1096                                         }
;    1097                                         lcd_gotoxy(0,1);               
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1098                                         if((j-2)>-1)
	LDS  R26,_j
	LDS  R27,_j+1
	SBIW R26,2
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	CP   R30,R26
	CPC  R31,R27
	BRGE _0xD9
;    1099                                         {
;    1100                                                  lcd_puts(line[j-2]);
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x16C
;    1101                                         }
;    1102                                         else
_0xD9:
;    1103                                         {
;    1104                                                  lcd_puts(line[buffersize+j-2]);                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x16C:
	SBIW R30,2
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1105                                         }
;    1106                                         lcd_gotoxy(0,2);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(2)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1107                                         if((j-1)>-1)
	LDS  R26,_j
	LDS  R27,_j+1
	SBIW R26,1
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	CP   R30,R26
	CPC  R31,R27
	BRGE _0xDB
;    1108                                         {
;    1109                                                  lcd_puts(line[j-1]);
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x16D
;    1110                                         }
;    1111                                         else
_0xDB:
;    1112                                         {
;    1113                                                  lcd_puts(line[buffersize+j-1]);                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x16D:
	SBIW R30,1
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1114                                         }
;    1115                                                                  
;    1116                                         lcd_gotoxy(0,3);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1117                                         lcd_puts(line[j]);
	LDS  R30,_j
	LDS  R31,_j+1
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1118                              //printf("%s \n\r",line[j]);
;    1119                              //printf("j is %i\n\r",j);
;    1120                              break;
	RJMP _0x91
;    1121                              
;    1122                              
;    1123                              case scrollup:
_0xD0:
	CPI  R30,LOW(0x5)
	LDI  R26,HIGH(0x5)
	CPC  R31,R26
	BREQ PC+2
	RJMP _0xEA
;    1124                         
;    1125                         
;    1126                         
;    1127                                 if (wasscroll==0)
	SBRC R2,1
	RJMP _0xDE
;    1128                                 { 
;    1129                                 //printf("wasscroll just got set");
;    1130                                       wasscroll=1;      
	SET
	BLD  R2,1
;    1131                                 
;    1132                                         istore=i;
	LDS  R30,_i
	LDS  R31,_i+1
	STS  _istore,R30
	STS  _istore+1,R31
;    1133                                         jstore=j;
	LDS  R30,_j
	LDS  R31,_j+1
	STS  _jstore,R30
	STS  _jstore+1,R31
;    1134                                         for(x=20-i;x!=0;x--)                 //this just fills the remainng of line with spaces
	LDS  R26,_i
	LDS  R27,_i+1
	LDI  R30,LOW(20)
	LDI  R31,HIGH(20)
	SUB  R30,R26
	SBC  R31,R27
	STS  _x,R30
	STS  _x+1,R31
_0xE0:
	LDS  R30,_x
	LDS  R31,_x+1
	SBIW R30,0
	BREQ _0xE1
;    1135                         {                                                                                                                                                                                                                                                                                                
;    1136                                 line[j][i]=0x20;
	LDS  R30,_j
	LDS  R31,_j+1
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	LDS  R26,_i
	LDS  R27,_i+1
	ADD  R26,R30
	ADC  R27,R31
	LDI  R30,LOW(32)
	ST   X,R30
;    1137                                 i++;
	LDS  R30,_i
	LDS  R31,_i+1
	ADIW R30,1
	STS  _i,R30
	STS  _i+1,R31
;    1138                                 ////printf("%i",i);   
;    1139                         }
	LDS  R30,_x
	LDS  R31,_x+1
	SBIW R30,1
	STS  _x,R30
	STS  _x+1,R31
	RJMP _0xE0
_0xE1:
;    1140                                 }
;    1141                                         
;    1142                                 
;    1143                              if(j>0)   
_0xDE:
	LDS  R26,_j
	LDS  R27,_j+1
	RCALL __CPW02
	BRGE _0xE2
;    1144                                     j--;
	LDS  R30,_j
	LDS  R31,_j+1
	SBIW R30,1
	RJMP _0x16E
;    1145                              else
_0xE2:
;    1146                                    j=buffersize-1;
	LDI  R30,LOW(6)
	LDI  R31,HIGH(6)
_0x16E:
	STS  _j,R30
	STS  _j+1,R31
;    1147                                     
;    1148                              lcd_clear();
	RCALL _lcd_clear
;    1149                                         
;    1150                                         if((j-3)>-1)
	LDS  R26,_j
	LDS  R27,_j+1
	SBIW R26,3
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	CP   R30,R26
	CPC  R31,R27
	BRGE _0xE4
;    1151                                         {
;    1152                                                  lcd_puts(line[j-3]);      
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x16F
;    1153                                         }
;    1154                                         else
_0xE4:
;    1155                                         {
;    1156                                                  lcd_puts(line[buffersize+j-3]);                                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x16F:
	SBIW R30,3
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1157                                         }
;    1158                                         lcd_gotoxy(0,1);               
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1159                                         if((j-2)>-1)
	LDS  R26,_j
	LDS  R27,_j+1
	SBIW R26,2
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	CP   R30,R26
	CPC  R31,R27
	BRGE _0xE6
;    1160                                         {
;    1161                                                  lcd_puts(line[j-2]);
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x170
;    1162                                         }
;    1163                                         else
_0xE6:
;    1164                                         {
;    1165                                                  lcd_puts(line[buffersize+j-2]);                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x170:
	SBIW R30,2
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1166                                         }
;    1167                                         lcd_gotoxy(0,2);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(2)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1168                                         if((j-1)>-1)
	LDS  R26,_j
	LDS  R27,_j+1
	SBIW R26,1
	LDI  R30,LOW(65535)
	LDI  R31,HIGH(65535)
	CP   R30,R26
	CPC  R31,R27
	BRGE _0xE8
;    1169                                         {
;    1170                                                  lcd_puts(line[j-1]);
	LDS  R30,_j
	LDS  R31,_j+1
	RJMP _0x171
;    1171                                         }
;    1172                                         else
_0xE8:
;    1173                                         {
;    1174                                                  lcd_puts(line[buffersize+j-1]);                               
	LDS  R30,_j
	LDS  R31,_j+1
	ADIW R30,7
_0x171:
	SBIW R30,1
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1175                                         }
;    1176                                                                  
;    1177                                         lcd_gotoxy(0,3);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;    1178                                         lcd_puts(line[j]);
	LDS  R30,_j
	LDS  R31,_j+1
	LDI  R26,LOW(21)
	LDI  R27,HIGH(21)
	RCALL __MULW12U
	SUBI R30,LOW(-_line)
	SBCI R31,HIGH(-_line)
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_puts
;    1179                              //printf("%s \n\r",line[j]);
;    1180                              //printf("j is %i\n\r",j);
;    1181                              break;        
;    1182                         default:
_0xEA:
;    1183                         break;
;    1184                 }
_0x91:
;    1185                         
;    1186              
;    1187       
;    1188       
;    1189 
;    1190       
;    1191 }
	ADIW R28,1
	RET
;    1192  unsigned char key;

	.DSEG
_key:
	.BYTE 0x1
;    1193 int comunicate (void)
;    1194 
;    1195 {

	.CSEG
_comunicate:
;    1196        key=getchar_kb();
	RCALL _getchar_kb
	STS  _key,R30
;    1197        if(key==0xfd)
	LDS  R26,_key
	CPI  R26,LOW(0xFD)
	BRNE _0xEB
;    1198        return 0;
	LDI  R30,LOW(0)
	LDI  R31,HIGH(0)
	RET
;    1199        else
_0xEB:
;    1200        
;    1201        if(key==0xff || key == 0xfe)
	LDS  R26,_key
	CPI  R26,LOW(0xFF)
	BREQ _0xEE
	CPI  R26,LOW(0xFE)
	BRNE _0xED
_0xEE:
;    1202         do_states(key);
	LDS  R30,_key
	ST   -Y,R30
	RCALL _do_states
;    1203        else
	RJMP _0xF0
_0xED:
;    1204        {
;    1205        if(key!=0 )
	LDS  R30,_key
	CPI  R30,0
	BREQ _0xF1
;    1206        {
;    1207         //putchar(key);
;    1208         //key=getchar();
;    1209         if(key==13)
	LDS  R26,_key
	CPI  R26,LOW(0xD)
	BRNE _0xF2
;    1210         {
;    1211                 //putchar(13);
;    1212                 putchar(10);
	LDI  R30,LOW(10)
	RJMP _0x172
;    1213         }
;    1214         else 
_0xF2:
;    1215         
;    1216         putchar(key);       
	LDS  R30,_key
_0x172:
	ST   -Y,R30
	RCALL _putchar
;    1217         //do_states(key);
;    1218        }
;    1219        } 
_0xF1:
_0xF0:
;    1220        //key=getchar();
;    1221        //if(key!=1)
;    1222        //{
;    1223        
;    1224        key=getchar();
	RCALL _getchar
	STS  _key,R30
;    1225        if(key==0x1b)
	LDS  R26,_key
	CPI  R26,LOW(0x1B)
	BREQ _0xF5
;    1226               {        
;    1227                 //while (key!=0x6d)
;    1228                 //{
;    1229                   //      key=getchar();
;    1230                 //}
;    1231        }
;    1232        else
;    1233        {
;    1234                if(key!=0 && key!=0xff && key!=0x0fe)
	CPI  R26,LOW(0x0)
	BREQ _0xF7
	CPI  R26,LOW(0xFF)
	BREQ _0xF7
	CPI  R26,LOW(0xFE)
	BRNE _0xF8
_0xF7:
	RJMP _0xF6
_0xF8:
;    1235                //printf("%c",key);
;    1236                {
;    1237                do_states(key);
	LDS  R30,_key
	ST   -Y,R30
	RCALL _do_states
;    1238                }
;    1239        }  
_0xF6:
_0xF5:
;    1240        
;    1241        return 1;
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	RET
;    1242     }

    .equ __lcd_direction=__lcd_port-1
    .equ __lcd_pin=__lcd_port-2
    .equ __lcd_rs=0
    .equ __lcd_rd=1
    .equ __lcd_enable=2
    .equ __lcd_busy_flag=7

	.DSEG
__base_y_G7:
	.BYTE 0x4
__lcd_x:
	.BYTE 0x1
__lcd_y:
	.BYTE 0x1
__lcd_maxx:
	.BYTE 0x1

	.CSEG
__lcd_delay_G7:
    ldi   r31,15
__lcd_delay0:
    dec   r31
    brne  __lcd_delay0
	RET
__lcd_ready:
    in    r26,__lcd_direction
    andi  r26,0xf                 ;set as input
    out   __lcd_direction,r26
    sbi   __lcd_port,__lcd_rd     ;RD=1
    cbi   __lcd_port,__lcd_rs     ;RS=0
__lcd_busy:
	RCALL __lcd_delay_G7
    sbi   __lcd_port,__lcd_enable ;EN=1
	RCALL __lcd_delay_G7
    in    r26,__lcd_pin
    cbi   __lcd_port,__lcd_enable ;EN=0
	RCALL __lcd_delay_G7
    sbi   __lcd_port,__lcd_enable ;EN=1
	RCALL __lcd_delay_G7
    cbi   __lcd_port,__lcd_enable ;EN=0
    sbrc  r26,__lcd_busy_flag
    rjmp  __lcd_busy
	RET
__lcd_write_nibble_G7:
    andi  r26,0xf0
    or    r26,r27
    out   __lcd_port,r26          ;write
    sbi   __lcd_port,__lcd_enable ;EN=1
	RCALL __lcd_delay_G7
    cbi   __lcd_port,__lcd_enable ;EN=0
	RCALL __lcd_delay_G7
	RET
__lcd_write_data:
    cbi  __lcd_port,__lcd_rd 	  ;RD=0
    in    r26,__lcd_direction
    ori   r26,0xf0 | (1<<__lcd_rs) | (1<<__lcd_rd) | (1<<__lcd_enable) ;set as output    
    out   __lcd_direction,r26
    in    r27,__lcd_port
    andi  r27,0xf
    ld    r26,y
	RCALL __lcd_write_nibble_G7
    ld    r26,y
    swap  r26
	RCALL __lcd_write_nibble_G7
    sbi   __lcd_port,__lcd_rd     ;RD=1
	ADIW R28,1
	RET
_lcd_write_byte:
	RCALL __lcd_ready
	LDD  R30,Y+1
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __lcd_ready
    sbi   __lcd_port,__lcd_rs     ;RS=1
	LD   R30,Y
	ST   -Y,R30
	RCALL __lcd_write_data
	ADIW R28,2
	RET
__lcd_read_nibble_G7:
    sbi   __lcd_port,__lcd_enable ;EN=1
	RCALL __lcd_delay_G7
    in    r30,__lcd_pin           ;read
    cbi   __lcd_port,__lcd_enable ;EN=0
	RCALL __lcd_delay_G7
    andi  r30,0xf0
	RET
_lcd_read_byte0_G7:
	RCALL __lcd_delay_G7
	RCALL __lcd_read_nibble_G7
    mov   r26,r30
	RCALL __lcd_read_nibble_G7
    cbi   __lcd_port,__lcd_rd     ;RD=0
    swap  r30
    or    r30,r26
	RET
_lcd_gotoxy:
	RCALL __lcd_ready
	LD   R30,Y
	LDI  R31,0
	SUBI R30,LOW(-__base_y_G7)
	SBCI R31,HIGH(-__base_y_G7)
	LD   R30,Z
	LDD  R26,Y+1
	ADD  R30,R26
	ST   -Y,R30
	RCALL __lcd_write_data
	LDD  R30,Y+1
	STS  __lcd_x,R30
	LD   R30,Y
	STS  __lcd_y,R30
	ADIW R28,2
	RET
_lcd_clear:
	RCALL __lcd_ready
	LDI  R30,LOW(2)
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __lcd_ready
	LDI  R30,LOW(15)
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __lcd_ready
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL __lcd_write_data
	LDI  R30,LOW(0)
	STS  __lcd_y,R30
	STS  __lcd_x,R30
	RET
_lcd_putchar:
    push r30
    push r31
    ld   r26,y
    set
    cpi  r26,10
    breq __lcd_putchar1
    clt
	LDS  R30,__lcd_x
	SUBI R30,-LOW(1)
	STS  __lcd_x,R30
	LDS  R30,__lcd_maxx
	LDS  R26,__lcd_x
	CP   R30,R26
	BRSH _0xFA
	__lcd_putchar1:
	LDS  R30,__lcd_y
	SUBI R30,-LOW(1)
	STS  __lcd_y,R30
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDS  R30,__lcd_y
	ST   -Y,R30
	RCALL _lcd_gotoxy
	brts __lcd_putchar0
_0xFA:
    rcall __lcd_ready
    sbi  __lcd_port,__lcd_rs ;RS=1
    ld   r26,y
    st   -y,r26
    rcall __lcd_write_data
__lcd_putchar0:
    pop  r31
    pop  r30
	ADIW R28,1
	RET
_lcd_puts:
    ldd  r31,y+1
    ld   r30,y
__lcd_puts0:
    ld   r26,z+
    tst  r26
    breq __lcd_puts1
    st   -y,r26    
    rcall _lcd_putchar
    rjmp __lcd_puts0
__lcd_puts1:
	ADIW R28,2
	RET
_lcd_putsf:
    ld   r30,y
    ldd  r31,y+1
__lcd_putsf0:
	lpm
    tst  r0
    breq __lcd_putsf1
    adiw r30,1
    st   -y,r0
    rcall _lcd_putchar
    rjmp __lcd_putsf0
__lcd_putsf1:
	ADIW R28,2
	RET
__long_delay_G7:
    clr   r26
    clr   r27
__long_delay0:
    sbiw  r26,1         ;2 cycles
    brne  __long_delay0 ;2 cycles
	RET
__lcd_init_write_G7:
    cbi  __lcd_port,__lcd_rd 	  ;RD=0
    in    r26,__lcd_direction
    ori   r26,0xf7                ;set as output
    out   __lcd_direction,r26
    in    r27,__lcd_port
    andi  r27,0xf
    ld    r26,y
	RCALL __lcd_write_nibble_G7
    sbi   __lcd_port,__lcd_rd     ;RD=1
	ADIW R28,1
	RET
_lcd_init:
    cbi   __lcd_port,__lcd_enable ;EN=0
    cbi   __lcd_port,__lcd_rs     ;RS=0
	LD   R30,Y
	STS  __lcd_maxx,R30
	SUBI R30,-LOW(128)
	__PUTB1MN __base_y_G7,2
	LD   R30,Y
	SUBI R30,-LOW(192)
	__PUTB1MN __base_y_G7,3
	RCALL __long_delay_G7
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL __lcd_init_write_G7
	RCALL __long_delay_G7
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL __lcd_init_write_G7
	RCALL __long_delay_G7
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL __lcd_init_write_G7
	RCALL __long_delay_G7
	LDI  R30,LOW(32)
	ST   -Y,R30
	RCALL __lcd_init_write_G7
	RCALL __long_delay_G7
	LDI  R30,LOW(40)
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __long_delay_G7
	LDI  R30,LOW(4)
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __long_delay_G7
	LDI  R30,LOW(133)
	ST   -Y,R30
	RCALL __lcd_write_data
	RCALL __long_delay_G7
    in    r26,__lcd_direction
    andi  r26,0xf                 ;set as input
    out   __lcd_direction,r26
    sbi   __lcd_port,__lcd_rd     ;RD=1
	RCALL _lcd_read_byte0_G7
	CPI  R30,LOW(0x5)
	BREQ _0xFB
	LDI  R30,LOW(0)
	RJMP _0x157
