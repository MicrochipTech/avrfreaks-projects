	RCALL _lcd_putchar
;     402                         lcd_putsf("  HEX UBRR U WANT");
	__POINTW1FN _0,52
	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     403                         lcd_gotoxy(0,2);
