	ST   -Y,R31
	ST   -Y,R30
	RCALL _lcd_putsf
;     436                         lcd_gotoxy(0,3);
	LDI  R30,LOW(0)
	ST   -Y,R30
	LDI  R30,LOW(3)
	ST   -Y,R30
	RCALL _lcd_gotoxy
;     437                         lcd_putchar(3);
