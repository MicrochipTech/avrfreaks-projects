         ;     662 
         ;     663 
         ;     664  
         ;     665 // Declare your global variables here
         ;     666 
         ;     667 void main(void)
         ;     668 {
          _main:
         ;     669 
         ;     670 // Input/Output Ports initialization
         ;     671 // Port A initialization
         ;     672 // Func0=In Func1=In Func2=In Func3=In Func4=In Func5=In Func6=In Func7=In 
         ;     673 // State0=T State1=T State2=T State3=T State4=T State5=T State6=T State7=T 
         ;     674 PORTA=0x00;
00088e 27ee      	CLR  R30
00088f bbeb      	OUT  0x1B,R30
         ;     675 DDRA=0xFF;
000890 efef      	LDI  R30,LOW(255)
000891 bbea      	OUT  0x1A,R30
         ;     676 
         ;     677 // Port B initialization
         ;     678 // Func0=In Func1=In Func2=In Func3=In Func4=In Func5=In Func6=In Func7=In 
         ;     679 // State0=T State1=T State2=T State3=T State4=T State5=T State6=T State7=T 
         ;     680 PORTB=0x00;
000892 27ee      	CLR  R30
000893 bbe8      	OUT  0x18,R30
         ;     681 DDRB=0xFF;
000894 efef      	LDI  R30,LOW(255)
000895 bbe7      	OUT  0x17,R30
         ;     682 
         ;     683 // Port C initialization
         ;     684 // Func0=In Func1=In Func2=In Func3=In Func4=In Func5=In Func6=In Func7=In 
         ;     685 // State0=T State1=T State2=T State3=T State4=T State5=T State6=T State7=T 
         ;     686 PORTC=0x00;
000896 27ee      	CLR  R30
000897 bbe5      	OUT  0x15,R30
         ;     687 DDRC=0x00;
000898 bbe4      	OUT  0x14,R30
         ;     688 
         ;     689 // Port D initialization
         ;     690 // Func0=In Func1=In Func2=In Func3=In Func4=In Func5=In Func6=In Func7=In 
         ;     691 // State0=T State1=T State2=T State3=T State4=T State5=T State6=T State7=T 
         ;     692 PORTD=0x00;
000899 bbe2      	OUT  0x12,R30
         ;     693 DDRD=0x00;
00089a bbe1      	OUT  0x11,R30
         ;     694 
         ;     695 // Timer/Counter 0 initialization
         ;     696 // Clock source: System Clock
         ;     697 // Clock value: 3,906 kHz
         ;     698 TCCR0=0x00;                    //Stop timer till after init_devices
00089b d1bc      	RCALL SUBOPT_0x1F
         ;     699 TCNT0=0xC6;                    // set timer to 10 msec
00089c ece6      	LDI  R30,LOW(198)
00089d bfe2      	OUT  0x32,R30
         ;     700 
         ;     701 // Timer/Counter 1 initialization
         ;     702 // Clock source: System Clock
         ;     703 // Clock value: Timer 1 Stopped
         ;     704 // Mode: Normal top=FFFFh
         ;     705 // OC1A output: Discon.
         ;     706 // OC1B output: Discon.
         ;     707 // Noise Canceler: Off
         ;     708 // Input Capture on Falling Edge
         ;     709 TCCR1A=0x00;
00089e 27ee      	CLR  R30
00089f bdef      	OUT  0x2F,R30
         ;     710 TCCR1B=0x00;
0008a0 bdee      	OUT  0x2E,R30
         ;     711 TCNT1H=0x00;
0008a1 bded      	OUT  0x2D,R30
         ;     712 TCNT1L=0x00;
0008a2 bdec      	OUT  0x2C,R30
         ;     713 OCR1AH=0x00;
0008a3 bdeb      	OUT  0x2B,R30
         ;     714 OCR1AL=0x00;
0008a4 bdea      	OUT  0x2A,R30
         ;     715 OCR1BH=0x00;
0008a5 bde9      	OUT  0x29,R30
         ;     716 OCR1BL=0x00;
0008a6 bde8      	OUT  0x28,R30
         ;     717 
         ;     718 // External Interrupt(s) initialization
         ;     719 // INT0: On
         ;     720 // INT0 Mode: Low level (changed from falling edge to be able to go in powerdown mode)
         ;     721 // INT1: Off
         ;     722 GIMSK=0x40;
0008a7 e4e0      	LDI  R30,LOW(64)
0008a8 bfeb      	OUT  0x3B,R30
         ;     723 MCUCR=0x00;
