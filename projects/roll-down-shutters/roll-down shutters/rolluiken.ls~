000435 d56a      	RCALL SUBOPT_0x2
000436 d618      	RCALL SUBOPT_0x1C
000437 dc68      	RCALL _update_rolluik
         ;     391      if (rising_edge_mask & mask)
          _0x4D:
000438   +  	__GETW1R 16,17
