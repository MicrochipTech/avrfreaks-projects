0000f1 f009      	BREQ _0x25
          _0x24:
0000f2 c070      	RJMP _0x23
          _0x25:
         ;     278  
         ;     279      {
         ;     280       if (blocked(nr))
0000f3 d8cd      	RCALL SUBOPT_0x8
0000f4 93ff      	PUSH R31
