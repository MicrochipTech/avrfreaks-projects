0000cd 93ef      	PUSH R30
0000ce d8f5      	RCALL SUBOPT_0x9
0000cf 91af      	POP  R26
0000d0 91bf      	POP  R27
0000d1 d8f6      	RCALL SUBOPT_0xA
0000d2 5fef      	SUBI R30,-LOW(1)
0000d3 93ec      	ST   X,R30
         ;     275       }
         ;     276       
         ;     277  if ((released) && (rolluik[nr].frametimer == 0) && ( rolluik[nr].keypresses_in_frame == 0))
          _0x21:
0000d4 d8f7      	RCALL SUBOPT_0xB
0000d5 f0e1      	BREQ _0x24
0000d6 d8ea      	RCALL SUBOPT_0x8
0000d7 93ff      	PUSH R31
0000d8 93ef      	PUSH R30
0000d9   +  	__POINTW1MN _rolluik,2
0000d9 eeef      	LDI  R30,LOW(_rolluik+0x2)
0000da e0f0      	LDI  R31,HIGH(_rolluik+0x2)
          	.ENDM
0000db 91af      	POP  R26
0000dc 91bf      	POP  R27
0000dd 93ff      	PUSH R31
0000de 93ef      	PUSH R30
0000df d8e4      	RCALL SUBOPT_0x9
