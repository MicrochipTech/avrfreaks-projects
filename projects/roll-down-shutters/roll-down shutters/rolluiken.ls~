         ;     230    
         ;     231                  
         ;     232 
         ;     233  void set_output (unsigned char adress, unsigned char data )
         ;     234  {
         
          	.CSEG
          _set_output:
         ;     235   i2c_start();
000080 da82      	RCALL _i2c_start
         ;     236   i2c_write(adress);
000081 d918      	RCALL SUBOPT_0x0
000082 dab1      	RCALL _i2c_write
         ;     237   i2c_write(data);
000083 d919      	RCALL SUBOPT_0x1
000084 daaf      	RCALL _i2c_write
         ;     238   i2c_stop();
000085 da8c      	RCALL _i2c_stop
         ;     239  }
000086 9622      	ADIW R28,2
000087 9508      	RET
         ;     240 
         ;     241 int read_input ( unsigned char adress)
         ;     242 {
          _read_input:
         ;     243  int data;
         ;     244  
         ;     245  i2c_start();
000088 db66      	RCALL __SAVELOCR2
         ;	adress -> Y+2
         ;	data -> R16,R17
000089 da79      	RCALL _i2c_start
         ;     246  i2c_write(adress | 1);
00008a 81ea      	LDD  R30,Y+2
00008b 60e1      	ORI  R30,1
00008c 93ea      	ST   -Y,R30
00008d daa6      	RCALL _i2c_write
         ;     247  data=i2c_read(0);
00008e d911      	RCALL SUBOPT_0x2
00008f da8c      	RCALL _i2c_read
000090 27ff      	CLR  R31
000091   +  	__PUTW1R 16,17
000091 2f0e      	MOV  R0x10,R30
000092 2f1f      	MOV  R0x11,R31
          	.ENDM
         ;     248  i2c_stop();
000093 da7e      	RCALL _i2c_stop
         ;     249  return data;
000094   +  	__GETW1R 16,17
000094 2fe0      	MOV  R30,R0x10
000095 2ff1      	MOV  R31,R0x11
          	.ENDM
000096 db5c      	RCALL __LOADLOCR2
000097 9623      	ADIW R28,3
000098 9508      	RET
         ;     250 }
         ;     251 
         ;     252 void init_i2c_devices(void)
         ;     253 {   
          _init_i2c_devices:
         ;     254  set_output(PCF8574A_1,0xff);       //set I/O to 1 for input mode
000099 d909      	RCALL SUBOPT_0x3
00009a d90b      	RCALL SUBOPT_0x4
         ;     255  delay_ms(50);
         ;     256  set_output(PCF8574A_2,0xff);       //set I/O to 1 for input mode
00009b d912      	RCALL SUBOPT_0x5
00009c d909      	RCALL SUBOPT_0x4
         ;     257  delay_ms(50);
         ;     258  set_output(PCF8574A_3,0xff);       //set I/O to 1 for input mode
00009d d913      	RCALL SUBOPT_0x6
00009e d907      	RCALL SUBOPT_0x4
         ;     259  delay_ms(50);
         ;     260 
         ;     261 }
00009f 9508      	RET
         ;     262  
         ;     263 void update_rolluik ( char nr,char pressed,char released)
         ;     264 {
          _update_rolluik:
         ;     265  char i;
         ;     266  temp=1;
0000a0 930a      	ST   -Y,R16
         ;	nr -> Y+3
         ;	pressed -> Y+2
         ;	released -> Y+1
         ;	i -> R16
0000a1 d912      	RCALL SUBOPT_0x7
         ;     267  k=0;
0000a2 2466      	CLR  R6
         ;     268    
         ;     269   
         ;     270  if (pressed)
0000a3 81ea      	LDD  R30,Y+2
0000a4 30e0      	CPI  R30,0
0000a5 f171      	BREQ _0x21
         ;     271      {
         ;     272       if (!rolluik[nr].keypresses_in_frame)
0000a6 d91a      	RCALL SUBOPT_0x8
0000a7 93ff      	PUSH R31
0000a8 93ef      	PUSH R30
0000a9   +  	__POINTW1MN _rolluik,3
0000a9 efe0      	LDI  R30,LOW(_rolluik+0x3)
0000aa e0f0      	LDI  R31,HIGH(_rolluik+0x3)
          	.ENDM
0000ab 91af      	POP  R26
0000ac 91bf      	POP  R27
0000ad 93ff      	PUSH R31
0000ae 93ef      	PUSH R30
0000af d914      	RCALL SUBOPT_0x9
0000b0 91af      	POP  R26
0000b1 91bf      	POP  R27
0000b2 d915      	RCALL SUBOPT_0xA
0000b3 30e0      	CPI  R30,0
0000b4 f481      	BRNE _0x22
         ;     273            {rolluik[nr].frametimer = 50;}        // 50* 10 msec  = 0.5 sec timeframe  
0000b5 d90b      	RCALL SUBOPT_0x8
0000b6 93ff      	PUSH R31
0000b7 93ef      	PUSH R30
0000b8   +  	__POINTW1MN _rolluik,2
0000b8 eeef      	LDI  R30,LOW(_rolluik+0x2)
0000b9 e0f0      	LDI  R31,HIGH(_rolluik+0x2)
          	.ENDM
0000ba 91af      	POP  R26
0000bb 91bf      	POP  R27
0000bc 93ff      	PUSH R31
0000bd 93ef      	PUSH R30
0000be d905      	RCALL SUBOPT_0x9
0000bf 91af      	POP  R26
0000c0 91bf      	POP  R27
0000c1 0fae      	ADD  R26,R30
0000c2 1fbf      	ADC  R27,R31
0000c3 e3e2      	LDI  R30,LOW(50)
0000c4 93ec      	ST   X,R30
         ;     274       ++rolluik[nr].keypresses_in_frame;
          _0x22:
0000c5 d8fb      	RCALL SUBOPT_0x8
0000c6 93ff      	PUSH R31
0000c7 93ef      	PUSH R30
0000c8   +  	__POINTW1MN _rolluik,3
0000c8 efe0      	LDI  R30,LOW(_rolluik+0x3)
