          	.DW  _message1
00000d 0080
          	.DW  0xE
00000e 000e
          	.DB  0x52,0x4F,0x4C,0x4C,0x55,0x49,0x4B,0x4D
00000f 4f52
000010 4c4c
000011 4955
000012 4d4b
          	.DB  0x4F,0x44,0x55,0x4C,0x45,0x0
000013 444f
000014 4c55
000015 0045
          __message2:
          	.DW  _message2
000016 008e
