         ;	mask -> R16,R17
         ;	i -> R18
000423 e001      	LDI  R16,1
000424 e010      	LDI  R17,0
000425 e020      	LDI  R18,LOW(0)
          _0x4B:
000426 3120      	CPI  R18,16
000427 f520      	BRSH _0x4C
         ;     388     {
         ;     389      if (falling_edge_mask & mask)
