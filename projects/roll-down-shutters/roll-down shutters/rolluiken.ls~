          __lcd_ready:
0008ce b3a4          in    r26,__lcd_direction
0008cf 70af          andi  r26,0xf                 ;set as input
0008d0 bba4          out   __lcd_direction,r26
0008d1 9aa9          sbi   __lcd_port,__lcd_rd     ;RD=1
0008d2 98a8          cbi   __lcd_port,__lcd_rs     ;RS=0
          __lcd_busy:
