000101 f161      	BREQ _0x26
         ;     281         {
         ;     282          rolluik[nr].status_reg = reset_blocked(nr);
000102 d8be      	RCALL SUBOPT_0x8
000103 93ff      	PUSH R31
000104 93ef      	PUSH R30
000105   +  	__POINTW1MN _rolluik,4
