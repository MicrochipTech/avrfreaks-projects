#include <mega8535.h>     


void main(void)
{

unsigned char s;  
int delay;
PORTA=0x00;
DDRA=0x0F;   

while (1)
      {      
           s=~PINA;
           PORTA=s;  
           for(delay=0;delay<10000;delay++)
           {   
                delay=delay;
           }
           
      };
}
